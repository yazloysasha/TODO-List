{
  "name": "todo-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "dev-web": "cross-env DEVICE=web npm run dev",
    "dev-android": "cross-env DEVICE=android npm run dev",
    "restart-web": "cross-env DEVICE=web npm run build && cross-env DEVICE=web npm run start",
    "restart-android": "cross-env DEVICE=android npm run build && npx cap sync android && npx cap open android",
    "add-android": "cross-env DEVICE=android npm run build && npx cap add android && npx capacitor-assets generate --iconBackgroundColor \"#1B1B1B\" --android"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/filesystem": "^6.0.1",
    "@capacitor/toast": "^6.0.2",
    "axios": "^1.7.7",
    "axios-hooks": "^5.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "next": "14.2.8",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18",
    "sass": "^1.78.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18",
    "typescript": "^5"
  }
}
